{
  "name": "mathis-page",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "format:write": "prettier {src,e2e,cypress}/**/*.{ts,json,md,scss} --write",
    "format:test": "prettier {src,e2e,cypress}/**/*.{ts,json,md,scss} --list-different",
    "cypress": "concurrently \"ng serve\" \"cypress open\"",
    "cypress-headless": "npx cypress --headless --browser chrome",
    "dev:ssr": "ng run mathis-page:serve-ssr",
    "serve:ssr": "node dist/mathis-page/server/main.js",
    "build:ssr": "ng build --prod && ng run mathis-page:server:production",
    "prerender": "ng run mathis-page:prerender"
  },
  "lint-staged": {
    "{src,e2e,cypress}/**/*.{ts,json,md,scss}": [
      "prettier --write",
      "git add"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.10",
    "@angular/cdk": "^14.1.0",
    "@angular/common": "^15.2.10",
    "@angular/compiler": "^15.2.10",
    "@angular/core": "^15.2.10",
    "@angular/flex-layout": "^14.0.0-beta.40",
    "@angular/forms": "^15.2.10",
    "@angular/material": "^14.1.0",
    "@angular/platform-browser": "^15.2.10",
    "@angular/platform-browser-dynamic": "^15.2.10",
    "@angular/platform-server": "^15.2.10",
    "@angular/router": "^15.2.10",
    "@nguniversal/express-engine": "^14.0.3",
    "browser-detect": "^0.2.27",
    "express": "^4.15.2",
    "rxjs": "~7.5.6",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.10",
    "@angular-eslint/builder": "^15.2.1",
    "@angular-eslint/eslint-plugin": "^15.2.1",
    "@angular-eslint/eslint-plugin-template": "^15.2.1",
    "@angular-eslint/schematics": "15.2.1",
    "@angular-eslint/template-parser": "^15.2.1",
    "@angular/cli": "^15.2.10",
    "@angular/compiler-cli": "^15.2.10",
    "@fortawesome/angular-fontawesome": "^0.11.1",
    "@fortawesome/fontawesome-free": "^6.1.2",
    "@fortawesome/fontawesome-svg-core": "^6.1.2",
    "@fortawesome/free-brands-svg-icons": "^6.1.2",
    "@fortawesome/free-solid-svg-icons": "^6.1.2",
    "@ngrx/data": "14.0.2",
    "@ngrx/effects": "14.0.2",
    "@ngrx/entity": "^14.0.2",
    "@ngrx/router-store": "14.0.2",
    "@ngrx/schematics": "^14.0.2",
    "@ngrx/store": "14.0.2",
    "@ngrx/store-devtools": "~14.0.2",
    "@nguniversal/builders": "^15.2.1",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "concurrently": "^6.0.0",
    "cypress": "^6.6.0",
    "eslint": "^8.28.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsdoc": "39.3.3",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "faker": "^5.4.0",
    "jasmine-core": "~4.3.0",
    "jasmine-marbles": "^0.9.2",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "prettier": "^1.19.1",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "typescript": "~4.8.0"
  }
}
