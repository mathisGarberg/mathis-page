{
  "name": "mathis-page",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "format:write": "prettier {src,e2e,cypress}/**/*.{ts,json,md,scss} --write",
    "format:test": "prettier {src,e2e,cypress}/**/*.{ts,json,md,scss} --list-different",
    "cypress": "concurrently \"ng serve\" \"cypress open\"",
    "cypress-headless": "npx cypress --headless --browser chrome",
    "dev:ssr": "ng run mathis-page:serve-ssr",
    "serve:ssr": "node dist/mathis-page/server/main.js",
    "build:ssr": "ng build --prod && ng run mathis-page:server:production",
    "prerender": "ng run mathis-page:prerender"
  },
  "lint-staged": {
    "{src,e2e,cypress}/**/*.{ts,json,md,scss}": [
      "prettier --write",
      "git add"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.0.1",
    "@angular/cdk": "^11.2.4",
    "@angular/common": "~12.0.1",
    "@angular/compiler": "~12.0.1",
    "@angular/core": "~12.0.1",
    "@angular/flex-layout": "^12.0.0-beta.34",
    "@angular/forms": "~12.0.1",
    "@angular/material": "^11.2.4",
    "@angular/platform-browser": "~12.0.1",
    "@angular/platform-browser-dynamic": "~12.0.1",
    "@angular/platform-server": "~12.0.1",
    "@angular/router": "~12.0.1",
    "@nguniversal/express-engine": "^11.2.1",
    "browser-detect": "^0.2.27",
    "express": "^4.15.2",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.0.1",
    "@angular-eslint/builder": "^12.0.0",
    "@angular-eslint/eslint-plugin": "^12.0.0",
    "@angular-eslint/eslint-plugin-template": "^12.0.0",
    "@angular-eslint/schematics": "12.0.0",
    "@angular-eslint/template-parser": "^12.0.0",
    "@angular/cli": "~12.0.1",
    "@angular/compiler-cli": "~12.0.1",
    "@fortawesome/angular-fontawesome": "^0.9.0",
    "@fortawesome/fontawesome-free": "^5.15.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.34",
    "@fortawesome/free-brands-svg-icons": "^5.15.2",
    "@fortawesome/free-solid-svg-icons": "^5.15.2",
    "@ngrx/data": "^11.0.1",
    "@ngrx/effects": "~11.0.1",
    "@ngrx/entity": "^11.0.1",
    "@ngrx/router-store": "~11.0.1",
    "@ngrx/schematics": "^11.0.1",
    "@ngrx/store": "~11.0.1",
    "@ngrx/store-devtools": "~11.0.1",
    "@nguniversal/builders": "^12.0.0",
    "@ngx-translate/core": "^12.0.0",
    "@ngx-translate/http-loader": "^4.0.0",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "4.23.0",
    "@typescript-eslint/parser": "4.23.0",
    "concurrently": "^6.0.0",
    "cypress": "^6.6.0",
    "eslint": "^7.26.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "faker": "^5.4.0",
    "jasmine-core": "~3.6.0",
    "jasmine-marbles": "^0.8.1",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "prettier": "^1.19.1",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "typescript": "~4.2.4"
  }
}
